<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>

		<ContentTargetFolders>.</ContentTargetFolders>
        <Product>Qualia.Umb.PackageMigration</Product>
        <PackageId>Qualia.Umb.PackageMigration</PackageId>
        <Title>Qualia.Umb.PackageMigration</Title>
		<Description>
			An umbraco cms package for migrating xml packages.
		</Description>
		<PackageProjectUrl>https://github.com/PagonisTasos/Qualia.Umb.PackageMigration</PackageProjectUrl>
		<PackageTags>umbraco plugin package; package migration</PackageTags>
        <RootNamespace>Qualia.Umb.PackageMigration</RootNamespace>
		<Version>1.1.0</Version>
		<Authors>Pagonis Tasos</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Umbraco.Cms.Web.Website" Version="9.1.2" />
        <PackageReference Include="Umbraco.Cms.Web.BackOffice" Version="9.1.2" />
    </ItemGroup>
</Project>
